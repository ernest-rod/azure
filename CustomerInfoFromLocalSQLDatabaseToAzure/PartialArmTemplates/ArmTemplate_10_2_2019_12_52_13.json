	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "CustomerInfoFromLocalSQLDatabaseToAzure"
		}
	},
	"variables": {
